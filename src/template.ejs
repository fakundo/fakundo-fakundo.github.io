<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover" />
  <!-- <title>Fakundo's useful links</title> -->
  <!-- <meta name="title" content="Fakundo's useful links" /> -->
  <!-- <meta name="description" content="Fakundo's list of useful links" /> -->

  <script async src="https://www.googletagmanager.com/gtag/js?id=<%= htmlWebpackPlugin.options.googleTagKey %>"></script>
  <script>
    window.dataLayer = window.dataLayer || [];
    function gtag() { dataLayer.push(arguments); }
    gtag('js', new Date());

    gtag('config', '<%= htmlWebpackPlugin.options.googleTagKey %>');
  </script>
</head>

<body>
  <!-- <header>
    <h1>Fakundo's Useful Links</h1>
  </header> -->
  <main>
    <ul>
      <% for(const { href, title } of htmlWebpackPlugin.options.links || []) { %>
        <li>
          <a href="<%= href %>"><%= title %></a>
        </li>
      <% } %>
    </ul>
  </main>
  <footer>
    2021 Â© Fakundo
  </footer>
</body>

</html>
